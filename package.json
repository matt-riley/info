{
  "name": "api.mattriley.info",
  "version": "0.0.1",
  "description": "Matt Riley Info API",
  "repository": "https://github.com/matt-riley/mattapps/tree/master/api.mattriley.info",
  "main": "lib/index.js",
  "scripts": {
    "commit": "npx git-cz",
    "prebuild": "npm run lint:fix",
    "build": "tsc",
    "getEnv": "node utils/getEnv.js",
    "prestart": "npm run getEnv",
    "predeploy": "npm run build && gcloud config set project mattriley-info",
    "deploy": "gcloud beta app deploy",
    "start": "node .",
    "prestart:dev": "npm run build",
    "start:dev": "node ./lib",
    "release": "standard-version",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "keywords": [],
  "author": "Matt Riley <matt@mattriley.me> (https://mattriley.io)",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "2.8.4",
    "@types/dotenv": "6.1.0",
    "@types/express": "4.16.1",
    "@types/express-enforces-ssl": "1.1.1",
    "@types/graphql": "14.0.7",
    "@types/helmet": "0.0.43",
    "@types/node": "11.11.6",
    "commitizen": "3.0.7",
    "cz-conventional-changelog": "2.1.0",
    "npm-run-all": "4.1.5",
    "standard-version": "5.0.2",
    "tslint": "5.14.0",
    "typescript": "3.3.4000"
  },
  "dependencies": {
    "@google-cloud/profiler": "1.0.2",
    "@google-cloud/storage": "2.4.3",
    "@google-cloud/trace-agent": "3.6.0",
    "apollo-datasource-rest": "0.3.2",
    "apollo-server-express": "2.4.8",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "express-enforces-ssl": "1.1.0",
    "firebase-admin": "7.1.1",
    "graphql": "14.1.1",
    "helmet": "3.16.0",
    "winston": "3.2.1"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": "6.x"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
