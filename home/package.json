{
  "name": "home",
  "description": "something, something, dangerzone",
  "version": "1.0.0",
  "main": "dist/server/index.js",
  "license": "MIT",
  "scripts": {
    "setup": "firebase use --add",
    "build:client": "next build src/client",
    "build:server": "babel src/server --out-dir dist/server --source-maps",
    "build": "npm run build:client && npm run build:server",
    "watch:client": "next src/client",
    "watch:server": "babel src/server --out-dir dist/server --source-maps --watch",
    "watch": "npm run watch:client & npm run watch:server",
    "serve": "NODE_ENV=production firebase serve --only functions,hosting",
    "predeploy": "rimraf dist/ && npm run build",
    "deploy": "NODE_ENV=production firebase deploy --only hosting,functions:indexNextjs"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "firebase-admin": "8.9.2",
    "firebase-functions": "3.3.0",
    "next": "9.2.2",
    "react": "16.13.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.6",
    "@babel/preset-env": "7.8.6",
    "firebase-functions-test": "0.2.0",
    "firebase-tools": "7.14.0",
    "rimraf": "3.0.2"
  }
}
